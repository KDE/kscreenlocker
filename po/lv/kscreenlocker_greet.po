#: fallbacktheme/Greeter.qml:59
msgid "Warning: Caps Lock on"
msgstr "Uzmanību: ir ieslēgts burtslēga taustiņš"

#: fallbacktheme/Greeter.qml:75
msgid "The session is locked"
msgstr "Sesija ir bloķēta"

#: fallbacktheme/Greeter.qml:76
msgid "The session has been locked by %1"
msgstr "Sesiju ir bloķējis %1"

#: fallbacktheme/Greeter.qml:100
msgid "(or place your fingerprint on the reader)"
msgstr "(vai novietojiet pirkstgalu uz pirkstu nospieduma lasītāja)"

#: fallbacktheme/Greeter.qml:106
msgid "(or use your smartcard)"
msgstr "(vai izmantojiet viedkarti)"

#: fallbacktheme/Greeter.qml:115
msgid "&Switch Users"
msgstr "&Pārslēgt lietotājus"

#: fallbacktheme/Greeter.qml:122
msgid "Un&lock"
msgstr "At&bloķēt"

#: fallbacktheme/Greeter.qml:134
msgid "Unlocking failed"
msgstr "Atbloķēšana neizdevās"

#: main.cpp:128
#, kde-format
msgid "Greeter for the KDE Plasma Workspaces Screen locker"
msgstr "KDE „Plasma“ darbvirsmas ekrāna bloķētāja sveicinātājs"

#: main.cpp:132
#, kde-format
msgid "Starts the greeter in testing mode"
msgstr "Palaiž sveicinātāju testēšanas režīmā"

#: main.cpp:135
#, kde-format
msgid "Starts the greeter with the selected shell theme (only in Testing mode)"
msgstr ""
"Palaiž sveicinātāju ar izvēlēto čaulas motīvu (tikai testēšanas režīmā)"

#: main.cpp:139
#, kde-format
msgid "Lock immediately, ignoring any grace time etc."
msgstr "Bloķēt nekavējoties. Ignorēt jebkādu gaidīšanas laiku u.tml."

#: main.cpp:141
#, kde-format
msgid "Delay till the lock user interface gets shown in milliseconds."
msgstr "Noildze bloķēšanas saskarnes parādīšanai (milisekundēs)."

#: main.cpp:144
#, kde-format
msgid "Don't show any lock user interface."
msgstr "Nerādīt bloķēšanas saskarni"

#: main.cpp:145
#, kde-format
msgid "Default to the switch user UI."
msgstr "Pēc noklusējuma rādīt lietotāju pārslēgšanas saskarni"

#: main.cpp:147
#, kde-format
msgid "File descriptor for connecting to ksld."
msgstr "„klsd“ savienojuma datnes deskriptors."
