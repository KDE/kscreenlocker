#: fallbacktheme/Greeter.qml:59
msgid "Warning: Caps Lock on"
msgstr "Xəbərdarlıq: Caps Lock açıq"

#: fallbacktheme/Greeter.qml:75
msgid "The session is locked"
msgstr "Sessiya kilidlənib"

#: fallbacktheme/Greeter.qml:76
msgid "The session has been locked by %1"
msgstr "Sessiya %1 tərəfindən kilidlənib"

#: fallbacktheme/Greeter.qml:100
msgid "(or place your fingerprint on the reader)"
msgstr "(və ya barmağınızı oxuyucuya yerləşdirin)"

#: fallbacktheme/Greeter.qml:106
msgid "(or use your smartcard)"
msgstr "(və ya smart kartdan istifadə edin)"

#: fallbacktheme/Greeter.qml:115
msgid "&Switch Users"
msgstr "&İstiifadəçini dəyişmək"

#: fallbacktheme/Greeter.qml:122
msgid "Un&lock"
msgstr "Ki&liddən çıxartmaq"

#: fallbacktheme/Greeter.qml:134
msgid "Unlocking failed"
msgstr "Kiliddən çıxarılmadı"

#: main.cpp:128
#, kde-format
msgid "Greeter for the KDE Plasma Workspaces Screen locker"
msgstr "KDE Plasma iş sahələri ekran kilidləyicisi ilə salamlaşma"

#: main.cpp:132
#, kde-format
msgid "Starts the greeter in testing mode"
msgstr "Salamlayıcını sınaq rejimində başlatmaq"

#: main.cpp:135
#, kde-format
msgid "Starts the greeter with the selected shell theme (only in Testing mode)"
msgstr ""
"Slamlayıcını seçilmiş üz qabığı mövzusu ilə başladır (yalnız sınaq rejimində)"

#: main.cpp:139
#, kde-format
msgid "Lock immediately, ignoring any grace time etc."
msgstr "Ekranı dərhal, gecikmə olmadan kilidləmək"

#: main.cpp:141
#, kde-format
msgid "Delay till the lock user interface gets shown in milliseconds."
msgstr "İstifadəçi interfeysi göstərilmədən öncə millisaniyərlə gecikmə."

#: main.cpp:144
#, kde-format
msgid "Don't show any lock user interface."
msgstr "Ekran kilidlənməsini göstərməmək."

#: main.cpp:146
#, kde-format
msgid "File descriptor for connecting to ksld."
msgstr "ksld ilə bağlantı üçün fayl deskriptoru"
