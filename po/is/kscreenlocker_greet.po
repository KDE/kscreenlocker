#: main.cpp:126
#, kde-format
msgid "Greeter for the KDE Plasma Workspaces Screen locker"
msgstr "Kveðjuviðmót fyrir KDE Plasma skjálæsinguna"

#: main.cpp:130
#, kde-format
msgid "Starts the greeter in testing mode"
msgstr "Byrjar kveðjuviðmótið í prufusniði"

#: main.cpp:133
#, kde-format
msgid "Starts the greeter with the selected theme (only in Testing mode)"
msgstr "Byrjar kveðjuviðmótið með valda þemana (aðeins í prufusniði)"

#: main.cpp:137
#, kde-format
msgid "Lock immediately, ignoring any grace time etc."
msgstr "Læsa strax, hunsa allan náðartíma o.s.frv."

#: main.cpp:139
#, kde-format
msgid "Delay till the lock user interface gets shown in milliseconds."
msgstr "Bið þar til læsiviðmótið birtist í millisekúndum."

#: main.cpp:142
#, kde-format
msgid "Don't show any lock user interface."
msgstr "Ekki sýna neitt læsiviðmót."

#: main.cpp:143
#, kde-format
msgid "Default to the switch user UI."
msgstr "Fara sjálfgefið í viðmót til að skipta um notanda."

#: main.cpp:145
#, kde-format
msgid "File descriptor for connecting to ksld."
msgstr "Skráarlýsir fyrir tengingu við ksld."
