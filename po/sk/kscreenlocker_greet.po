#: fallbacktheme/Greeter.qml:58
msgid "Warning: Caps Lock on"
msgstr "Upozornenie: zapnutý Caps Lock"

#: fallbacktheme/Greeter.qml:74
msgid "The session is locked"
msgstr "Sedenie je zamknuté"

#: fallbacktheme/Greeter.qml:75
msgid "The session has been locked by %1"
msgstr "Sedenie zamkol %1"

#: fallbacktheme/Greeter.qml:99
msgid "(or place your fingerprint on the reader)"
msgstr ""

#: fallbacktheme/Greeter.qml:105
msgid "(or use your smartcard)"
msgstr ""

#: fallbacktheme/Greeter.qml:114
msgid "&Switch Users"
msgstr "Prepnúť používateľov"

#: fallbacktheme/Greeter.qml:121
msgid "Un&lock"
msgstr "Odomknúť"

#: fallbacktheme/Greeter.qml:133
msgid "Unlocking failed"
msgstr "Odomknutie zlyhalo"

#: main.cpp:123
#, kde-format
msgid "Greeter for the KDE Plasma Workspaces Screen locker"
msgstr "Uvítač pre zamykač obrazovky KDE pracovnej plochy Plasma"

#: main.cpp:127
#, kde-format
msgid "Starts the greeter in testing mode"
msgstr "Spustí uvítač v testovacom režime"

#: main.cpp:130
#, fuzzy, kde-format
#| msgid "Starts the greeter with the selected theme (only in Testing mode)"
msgid "Starts the greeter with the selected shell theme (only in Testing mode)"
msgstr "Spustí uvítač s vybranou témou (iba v testovacom režime)"

#: main.cpp:134
#, kde-format
msgid "Lock immediately, ignoring any grace time etc."
msgstr "Zamknúť okamžite, ignorovať milostný čas atď."

#: main.cpp:136
#, kde-format
msgid "Delay till the lock user interface gets shown in milliseconds."
msgstr ""
"Oneskorenie, kým sa zobrazí zamykacie používateľské rozhranie v "
"milisekundách."

#: main.cpp:139
#, kde-format
msgid "Don't show any lock user interface."
msgstr "Nezobraziť žiadne zamykacie používateľské rozhranie."

#: main.cpp:141
#, kde-format
msgid "File descriptor for connecting to ksld."
msgstr "Popisovač súboru na pripojenie k ksld."

